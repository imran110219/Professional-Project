<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<Pane prefHeight="429.0" prefWidth="748.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <Label fx:id="lblSymmteric" layoutX="14.0" layoutY="14.0" text="Symmteric" textFill="#23151b">
            <font>
                <Font size="24.0"/>
            </font>
        </Label>
        <Label layoutX="18.0" layoutY="91.0" text="Input">
            <font>
                <Font size="16.0"/>
            </font>
        </Label>
        <TextArea fx:id="inputText" layoutX="76.0" layoutY="91.0" prefHeight="68.0" prefWidth="639.0"/>
        <Label layoutX="21.0" layoutY="180.0" text="Key">
            <font>
                <Font size="16.0"/>
            </font>
        </Label>
        <TextField fx:id="keyText" layoutX="76.0" layoutY="180.0" prefHeight="25.0" prefWidth="639.0"/>
        <Label layoutX="12.0" layoutY="223.0" text="Output">
            <font>
                <Font size="16.0"/>
            </font>
        </Label>
        <TextArea fx:id="outputText" editable="false" layoutX="76.0" layoutY="223.0" prefHeight="68.0"
                  prefWidth="639.0"/>
        <Button fx:id="btnGenerateKey" layoutX="76.0" layoutY="317.0" mnemonicParsing="false" onAction="#generateKey"
                text="Generate Key"/>
        <Button fx:id="btnEncrypt" layoutX="189.0" layoutY="317.0" mnemonicParsing="false" onAction="#doEncrypt"
                text="Encrypt"/>
        <Button fx:id="btnDecrypt" layoutX="272.0" layoutY="317.0" mnemonicParsing="false" onAction="#doDecrypt"
                text="Decrypt"/>
    </children>
</Pane>
