<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<Pane prefHeight="398.0" prefWidth="748.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.sadman.controller.RSAController">
    <children>
        <Label layoutX="14.0" layoutY="14.0" text="RSA" textFill="#23151b">
            <font>
                <Font size="24.0" />
            </font>
        </Label>
        <Label layoutX="18.0" layoutY="91.0" text="Input">
            <font>
                <Font size="16.0" />
            </font>
        </Label>
        <TextArea fx:id="inputText" layoutX="76.0" layoutY="91.0" prefHeight="68.0" prefWidth="639.0" />
        <Label layoutX="5.0" layoutY="177.0" text="Private Key">
            <font>
                <Font size="14.0" />
            </font>
        </Label>
        <TextField fx:id="privateKeyText" layoutX="76.0" layoutY="174.0" prefHeight="25.0" prefWidth="639.0" />
        <Label layoutX="5.0" layoutY="215.0" text="Public Key">
            <font>
                <Font size="14.0" />
            </font>
        </Label>
        <TextField fx:id="publicKeyText" layoutX="73.0" layoutY="212.0" prefHeight="25.0" prefWidth="639.0" />
        <Label layoutX="12.0" layoutY="251.0" text="Output">
            <font>
                <Font size="16.0" />
            </font>
        </Label>
        <TextArea fx:id="outputText" editable="false" layoutX="76.0" layoutY="251.0" prefHeight="68.0" prefWidth="639.0" />
        <Button fx:id="btnGenerateKey" layoutX="76.0" layoutY="334.0" mnemonicParsing="false" onAction="#generateKey" text="Generate Key" />
        <Button fx:id="btnEncrypt" layoutX="189.0" layoutY="334.0" mnemonicParsing="false" onAction="#doEncrypt" text="Encrypt" />
        <Button fx:id="btnDecrypt" layoutX="272.0" layoutY="334.0" mnemonicParsing="false" onAction="#doDecrypt" text="Decrypt" />

    </children>
</Pane>
